  "description": "Analytics for SRG mobile applications",
  "homepage": "https://bitbucket.org/rtsmb/srganalytics-ios",
  "license": {
    "type": "N/A"
  },
  "authors": {
    "Cédric Foellmi": "cedric.foellmi@hortis.ch",
